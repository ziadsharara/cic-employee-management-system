{
  "_version": "1.76.0",
  "sap.app": {
    "id": "cic.cictrial",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "b025c135-4bed-41f1-a6c1-75847562f6f8"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cic.cictrial.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "viewPath": "cic.cictrial.view",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "EmployeeList",
          "target": "EmployeeList"
        },
        {
          "pattern": "detail/{EmployeeID}",
          "name": "EmployeeDetail",
          "target": "EmployeeDetail"
        },
        {
          "pattern": "create",
          "name": "EmployeeCreate",
          "target": "EmployeeCreate"
        }
      ],
      "targets": {
        "EmployeeList": {
          "viewName": "EmployeeList",
          "viewId": "EmployeeList",
          "viewLevel": 1
        },
        "EmployeeDetail": {
          "viewName": "EmployeeDetail",
          "viewId": "EmployeeDetail",
          "viewLevel": 2
        },
        "EmployeeCreate": {
          "viewName": "cic.cictrial.view.EmployeeCreate",
          "viewId": "EmployeeCreate",
          "viewLevel": 2,
          "viewType": "XML"
        }
      }
    },
    "rootView": {
      "viewName": "cic.cictrial.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
